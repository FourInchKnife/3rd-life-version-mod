{
  "schemaVersion": 1,
  "id": "ryleu-smv",
  "version": "${version}",

  "name": "Server Modpack Version",
  "description": "Sets the version for ryleu's modpack.",
  "authors": [
    "ryleu"
  ],
  "contact": {
    "sources": "https://github.com/ryleu/Minecraft-Server-Version-Mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/ryleu-smv/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.ryleu.smv.RyleuSMV"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "*",
    "advanced_runtime_resource_pack": ">=0.3.11"
  },

  "breaks": {
    "wurst": "*",
    "meteor-client": "*",
    "inertia": "*",
    "ares": "*",
    "astolfo": "*"
  },

  "overrides": {
    "ferritecore": {
      "-depends": {
        "minecraft": ""
      }
    },
    "hydrogen": {
      "-depends": {
        "minecraft": ""
      }
    },
    "lambdynlights": {
      "-depends": {
        "minecraft": ""
      }
    },
    "modmenu": {
      "-depends": {
        "minecraft": ""
      }
    },
    "requiem": {
      "-depends": {
        "minecraft": ""
      }
    },
    "sodium": {
      "-depends": {
        "minecraft": ""
      }
    },
    "starlight": {
      "-depends": {
        "minecraft": ""
      }
    }
  }
}
